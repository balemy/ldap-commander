{
    "name": "yiisoft/app",
    "type": "project",
    "description": "Yii 3 application template",
    "keywords": [
        "yii3",
        "app"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "./cmda serve"
        ],
        "post-install-cmd": [
            "Balemy\\LdapCommander\\Installer::postUpdate",
            "Balemy\\LdapCommander\\Installer::copyEnvFile"
        ],
        "post-update-cmd": [
            "Balemy\\LdapCommander\\Installer::postUpdate",
            "Balemy\\LdapCommander\\Installer::copyEnvFile"
        ],
        "post-create-project-cmd": [
            "Balemy\\LdapCommander\\Installer::copyEnvFile"
        ],
        "test": "codecept run",
        "test-watch": "phpunit-watcher watch"
    },
    "require": {
        "php": "^8.2",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pdo_sqlite": "*",
        "cebe/markdown": "^1.2@dev",
        "directorytree/ldaprecord": "^3.3",
        "doctrine/collections": "^2.0",
        "fakerphp/faker": "^1.14",
        "httpsoft/http-message": "^1.0.5",
        "npm-asset/bootstrap-table": "^1.21",
        "npm-asset/font-awesome": "^6.2",
        "npm-asset/jquery": "^3.6",
        "npm-asset/select2": "^4.0",
        "npm-asset/select2-bootstrap-5-theme": "^1.3",
        "nyholm/psr7": "^1.5",
        "oomphinc/composer-installers-extender": "^2.0",
        "php-http/guzzle7-adapter": "^1.0",
        "psr/container": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^3.0",
        "symfony/console": "^6.0",
        "vlucas/phpdotenv": "^5.3",
        "yiisoft/access": "^2.0",
        "yiisoft/aliases": "^3.0",
        "yiisoft/assets": "^5.1",
        "yiisoft/auth": "^3.1",
        "yiisoft/config": "^1.6",
        "yiisoft/cookies": "^1.2",
        "yiisoft/csrf": "^2.2",
        "yiisoft/data": "dev-master as 1.0",
        "yiisoft/definitions": "^3.4",
        "yiisoft/di": "^1.4",
        "yiisoft/error-handler": "^4.1",
        "yiisoft/factory": "^1.3",
        "yiisoft/files": "^2.0",
        "yiisoft/form": "^1.4",
        "yiisoft/form-model": "^1.0",
        "yiisoft/html": "^3.11",
        "yiisoft/http": "^1.2",
        "yiisoft/i18n": "^1.1",
        "yiisoft/injector": "^1.2",
        "yiisoft/log": "^2.1",
        "yiisoft/log-target-file": "^3.0",
        "yiisoft/router-fastroute": "^4.0.1",
        "yiisoft/router": "^4.0",
        "yiisoft/security": "^1.0",
        "yiisoft/session": "^3.0",
        "yiisoft/translator": "^3.1",
        "yiisoft/translator-message-php": "^1.1",
        "yiisoft/user": "^2.3",
        "yiisoft/validator": "^2.5",
        "yiisoft/var-dumper": "^1.7",
        "yiisoft/view": "^12.2",
        "yiisoft/widget": "^2.2",
        "yiisoft/bootstrap5": "^1.0",
        "yiisoft/yii-console": "^2.3",
        "yiisoft/yii-event": "^2.1",
        "yiisoft/yii-http": "^1.0",
        "yiisoft/yii-middleware": "^1.1",
        "yiisoft/yii-runner-console": "^2.2",
        "yiisoft/yii-runner-http": "^3.2",
        "yiisoft/yii-view-renderer": "^7.3"
    },
    "require-dev": {
        "codeception/c3": "^2.9",
        "codeception/codeception": "^5.3",
        "codeception/module-asserts": "^3.2",
        "codeception/module-cli": "^2.0",
        "codeception/module-phpbrowser": "^3.0",
        "codeception/module-webdriver": "^3.2",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^6.8",
        "spatie/phpunit-watcher": "^1.23",
        "yiisoft/translator-extractor": "^2.0"
    },
    "replace": {
        "yiisoft/translator-formatter-simple": "*"
    },
    "autoload": {
        "psr-4": {
            "Balemy\\LdapCommander\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Balemy\\LdapCommander\\Tests\\": "tests"
        }
    },
    "extra": {
        "branc<h>-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin-file": "configuration.php",
        "installer-types": [
            "npm-asset"
        ],
        "installer-paths": {
            "./node_modules/{$name}": [
                "type:npm-asset"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "codeception/c3": true,
            "oomphinc/composer-installers-extender": true,
            "composer/installers": true,
            "composer/package-versions-deprecated": true,
            "infection/extension-installer": true,
            "yiisoft/config": true,
            "php-http/discovery": true
        }
    }
}
